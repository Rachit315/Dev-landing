{
  "version": 2,
  "builds": [
    {
      "src": "react-pixel-perfect-showcase/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/react-pixel-perfect-showcase/dist/$1"
    }
  ]
}
