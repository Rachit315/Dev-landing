{
  "name": "landing-page-full-stack",
  "version": "1.0.0",
  "description": "Full-stack landing page with React frontend and Express backend",
  "main": "api/index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "pnpm --filter vite_react_shadcn_ts run dev",
    "dev:server": "cd server && pnpm start",
    "build": "pnpm --filter vite_react_shadcn_ts run build",
    "build:client": "pnpm --filter vite_react_shadcn_ts run build",
    "start": "node api/index.js",
    "preview": "pnpm --filter vite_react_shadcn_ts run preview",
    "deploy": "node deploy.js"
  },
  "keywords": ["react", "express", "mongodb", "vercel", "fullstack"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "mongodb": "^6.18.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
